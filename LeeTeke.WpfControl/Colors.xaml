    <Style x:Key="LeeFocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Rectangle
                        Margin="0"
                        Stroke="{DynamicResource LeeBrush_Theme}"
                        StrokeThickness="1">
                        <Rectangle.Effect>
                            <DropShadowEffect
                                BlurRadius="8"
                                Direction="270"
                                Opacity=".2"
                                RenderingBias="Performance"
                                ShadowDepth="1.5"
                                Color="{DynamicResource LeeColor_Theme}" />
                        </Rectangle.Effect>
                    </Rectangle>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>




